{
  "name": "trail-map",
  "version": "1.0.0",
  "author": "",
  "dependencies": {
    "aws-sdk": "^2.338.0",
    "body-parser": "latest",
    "connect": "^3.6.6",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "path": "^0.12.7",
    "supertest": "^3.3.0"
  },
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha",
    "commit": "commit-wizard"
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  },
  "config": {
    "pre-git": {
      "enabled": true,
      "pre-commit": [
        "npm test"
      ],
      "pre-push": [
        "rm -rf node_modules",
        "npm install",
        "npm test"
      ],
      "post-commit": [
        "echo \"Test ran successfully\""
      ],
      "post-checkout": [],
      "post-merge": []
    }
  },
  "devDependencies": {
    "pre-git": "^3.17.1",
    "nyc": "^13.1.0"
  }
}
